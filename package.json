    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run webpack-dev-server\"",
    "dev-server": "NODE_ENV=development nodemon --exec babel-node ./bin/server.js",
    "webpack-dev-server": "NODE_ENV=development babel-node ./bin/webpack-dev-server.js",
    "start": "concurrently --kill-others \"npm run start-server\"",
    "start-server": "NODE_ENV=production babel-node ./bin/server.js",
    "lint": "rm -rf ./lint && mkdir lint && eslint ./shared/ --ext .js --ext .jsx  --ignore-pattern .eslintignore --cache -o ./lint/results.txt -f unix --quiet",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha mochaSetup.js './test/**/**/*.test.@(js|jsx)' --compilers js:babel-register --require babel-polyfill --recursive",
    "test": "NODE_ENV=test mocha mochaSetup.js './test/**/**/*.test.@(js|jsx)' --compilers js:babel-register --require babel-polyfill --recursive",
    "debug": "NODE_ENV=test mocha debug mochaSetup.js './test/**/**/*.test.@(js|jsx)' --compilers js:babel-register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "ci:": "npm run lint && npm test"
  },
  "keywords": [
    "React",
    "Redux",
    "Isomorphic"
  ],
  "author": "Art Richards (art@trucktech.in)",
